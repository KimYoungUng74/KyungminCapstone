<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<!-- [DB연결 사용법] 8. 만든 DTO와 Mapper정의하기 -->
	<typeAliases>
		<!-- <typeAlias type="com.spring.travel.dto.UserDTO" alias="UserDTO"></typeAlias> -->
		<typeAlias type="com.spring.banzzack.dto.UserDTO" alias="UserDTO"></typeAlias>
		<typeAlias type="com.spring.banzzack.dto.ProductDTO" alias="ProductDTO"></typeAlias>
		<typeAlias type="com.spring.banzzack.dto.VirtualDTO" alias="VirtualDTO"></typeAlias>
		<typeAlias type="com.spring.banzzack.dto.OrderDTO" alias="OrderDTO"></typeAlias>
		<typeAlias type="com.spring.banzzack.dto.MyOrderDTO" alias="MyOrderDTO"></typeAlias>
		<typeAlias type="com.spring.banzzack.dto.ReviewDTO" alias="ReviewDTO"></typeAlias>
	</typeAliases>
	
	
	<mappers>
		<!-- <mapper resource="mappers/UserMapper.xml" /> -->
		<mapper resource="mappers/UserMapper.xml" />
		<mapper resource="mappers/ProductMapper.xml" />
	</mappers>
</configuration>
